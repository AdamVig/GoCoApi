  "description": "API server for the GoCo Student app.",
  "main": "index.js",
  "dependencies": {
    "browser-agents": "0.1.0",
    "bunyan": "1.8.4",
    "cheerio": "0.19.0",
    "forecast.io-bluebird": "0.0.1",
    "moment": "2.11.1",
    "nano": "6.2.0",
    "request-promise": "2.0.0",
    "restify": "4.1.0"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "chai-json-schema": "1.2.0",
    "chalk": "1.1.3",
    "eslint": "2.13.1",
    "eslint-config-eslint": "3.0.0",
    "loadtest": "1.4.3",
    "mocha": "2.4.5",
    "nodemon": "1.10.2",
    "pm2": "2.4.2",
    "request-debug": "0.2.0",
    "yargs": "4.7.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adamvig/gocoapi.git"
  },
